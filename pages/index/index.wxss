.ResumeContainer {
  
  background: url("data:image/jpeg;base64,") repeat;
  font-size: 15px;
  font-family: "Helvetica Neue For Number",
                 -apple-system,
                 BlinkMacSystemFont,
                 "Segoe UI",
                 Roboto,
                 "PingFang SC",
                 "Hiragino Sans GB",
                 "Microsoft YaHei",
                 "Helvetica Neue",
                 Helvetica,
                 Arial,
                 sans-serif;
}

.overlay {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background: rgba(48, 53, 56, 0.8);
  z-index: 2000;
}

.overlayText {
  z-index: 2001;
  position: absolute;
  top: 48%;
  color: #fff;
}

.link {
  color: #4395ff;
}

.fontBold {
  font-weight: 900;
}

.hr {
  border-top: 1px solid #3d3b3b;
  height: 1px;
  margin: 10px;
}

.banner {
  position: relative;
}

.banner .bannerBg {
  width: 100%;
  height: 300px;
}

.banner .bannerOverlay {
  width: 100%;
  height: 300px;
  position: absolute;
  top: 0;
  left: 0;
  background: rgba(41, 41, 41, 0.2);
  z-index: 1;
}

.banner .info {
  color: #fff;
  position: absolute;
  z-index: 3;
  text-align: center;
  width: 100%;
  top: 85px;
}

.banner .titleResume {
  padding-bottom: 4px;
  margin-bottom: 5px;
  font-size: 1.7em;
  font-weight: 900;
}

.banner .titleResume::after {
  border-bottom: 2px solid #fff;
  content: ' ';
  width: 100px;
  height: 2px;
  display: block;
  margin-left: 38%;
}

.banner .enName {
  font-size: 1.2em;
  color:lime;
}

.banner .title {
  font-size: 1.2em;
}

.banner .head {
  position: absolute;
  top: 200px;
  left: 40%;
  width: 80px;
  height: 80px;
  border: 2px solid #fff;
  border-radius: 50%;
  z-index: 4;
}

.area {
  padding: 10px;
  color: #000;
  margin-top: 10px;
}

.area>.title {
  padding-bottom: 10px;
  border-bottom: 1px solid #3d3b3b;
  margin-bottom: 10px;
  font-size: 1.3em;
}

.area>.title::before {
  content: ' ';
  display: inline-block;
  border-left: 4px solid #3d3b3b;
  width: 1px;
  height: 20px;
  margin-left: 5px;
  margin-top: -4px;
  vertical-align: middle;
}

.aboutMe {
}

.aboutMe .iconArea, .contactList .iconArea {
  margin-bottom: 10px;
}

.aboutMe .iconArea image, .contactList .haveData  .iconArea  image {
  width: 20px;
  height: 20px;
  vertical-align: middle;
  margin-right: 10px;
}

.accordion .accordionItem {
  margin-bottom: 15px;
  padding: 5px;
  border: 1px solid #666161;
  border-radius: 4px;
}

.accordion .accordionItem .title {
  display: flex;
  padding: 5px;
}

.accordion .accordionItem .title text {
  flex: 1;
}

.accordion .accordionItem .title::after {
  content: ' ';
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  transition: 0.5s all;
}

.accordion .accordionItem .title.isShow::after {
  transform: rotate(90deg);
}

.accordion .accordionItem .contentWrap {
  margin-top: 10px;
  animation: all 0.7s;
  min-height: 100px;
}

.accordion .accordionItem .content {
  line-height: 30px;
  padding: 5px;
}

.accordion .accordionItem .content .detail {
  margin: 20px 0;
}

.skillList .content view {
  padding: 5px;
}

.skillList .content view:first-child, .starList .content view:first-child {
  padding-top: 0;
}

.starList .content view {
  padding: 5px;
  display: flex;
}

.starList .content view text {
  margin-right: 10px;
  width: 100px;
}

.starList .content view image {
  width: 20px;
  height: 20px;
  vertical-align: middle;
  margin-right: 5px;
}

.contactList .noData {
  text-align: center;
}

.contactList .noData image {
  width: 100px;
  height: 100px;
}

.powerBy {
  text-align: center;
  padding-bottom: 20px;
}

.button image {
  height: 26px;
  width: 89px;
  vertical-align: middle;
  margin-right: 5px;
}

.swiper_box {
  width: 100%;
  height:562.5rpx;
}